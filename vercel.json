{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "functions": {
            "api/listings": {
              "includeFiles": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx", "prisma/**/*"]
            }
          }
        }
      },
      {
        "src": "prisma/schema.prisma",
        "use": "@prisma/cli",
        "config": {
          "debug": true
        }
      }
    ]
  }
  